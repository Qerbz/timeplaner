// This is your Prisma schema file,
// learn more about it in the docs https//pris.ly/d/prisma-schema

generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "sqlite"
    url      = env("DATABASE_URL")
}

model Program {
    programId               String         @id
    title                   String
    studyprogCode           String
    studyprogName           String
    studyprogStudyLevel     String     
    studyprogStudyLevelCode Float      

    @@index([programId])
}

model Course {
    courseId                String  @id
    name                    String
    ownerId                 Int?
    showtype                Int?
    detailtype              String?
    name_en                 String?
    name_nn                 String?
    coursetype              String?
    tpsort                  String?
    showdiscipline          Int?
    campusid                String?
    yearfrom_und            Int?
    seasonfrom_und          String?
    yearto_und              String?
    seasonto_und            String?
    yearfrom_ex             Int?
    seasonfrom_ex           String?
    yearto_ex               String?
    seasonto_ex             String?
    departmentid_secondary  Int?
    create_activity_zoom    Int?
    authorized_netgroups    String?
    tpn_copy_daytime        String?
    nofterms                Int?
    terminnr                Int?
    fullname                String?
    fullname_en             String?
    fullname_nn             String?
    idtermin                String?

    @@index([courseId])
}

model StudyPlan {
    studyPlanId     String  @id
    json_data       String

    @@index([studyPlanId])
}